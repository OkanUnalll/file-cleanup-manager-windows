{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "GCC Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/program",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
            // "setupCommands": [
            //     {
            //         "description": "Enable pretty-printing for gdb",
            //         "text": "-enable-pretty-printing",
            //         "ignoreFailures": true
            //     }
            // ],
            "preLaunchTask": "build",
            "logging": {
                "trace": true,
                "traceResponse": true,
                "engineLogging": true
            },
            "targetArchitecture": "x64",
            "launchCompleteCommand": "exec-run",
            "debugServerPath": "",
            "debugServerArgs": "",
            "serverStarted": "",
            "filterStdout": false,
            "filterStderr": false,
            "serverLaunchTimeout": 10000,
            "internalConsoleOptions": "openOnSessionStart",
        }
    ]
}
